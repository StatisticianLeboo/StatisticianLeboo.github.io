library(likert)
library(corrplot)
secure<-survey_data %>%
select(`What's your course`, `How difficult was it to find industrial attachment`) %>%
drop_na()
View(secure)
esquisse:::esquisser()
ggplot(secure) +
aes(x = `How difficult was it to find industrial attachment`,fill = `What's your course`) +
geom_bar(position = "dodge") +
scale_fill_hue(direction = 1) +
labs(x = "Response",y = "Count",
title = "Difficulty in Securing Attachmnet",
fill = "Course") +
scale_y_continuous(n.breaks=10)+
theme_bw() +
theme(legend.position = "top",
plot.title = element_text(face = "bold.italic",
hjust = 0.5))+
scale_fill_tableau()
