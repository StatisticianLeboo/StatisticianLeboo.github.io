  </span><span class="n">filter</span><span class="p">(</span><span class="n">between</span><span class="w"> </span><span class="p">(</span><span class="n">age</span><span class="p">,</span><span class="m">15</span><span class="p">,</span><span class="m">49</span><span class="p">))</span><span class="w"> </span><span class="o">%&gt;%</span><span class="w"> 
  </span><span class="n">mutate</span><span class="p">(</span><span class="n">Aware</span><span class="o">=</span><span class="nf">as.numeric</span><span class="p">(</span><span class="n">if_else</span><span class="p">(</span><span class="n">heard_female_sterilization</span><span class="o">|</span><span class="n">heard_male_sterilization</span><span class="o">|</span><span class="n">heard_implants</span><span class="o">|</span><span class="n">heard_IUD</span><span class="o">|</span><span class="w">
